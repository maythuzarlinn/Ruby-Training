<h3 class="form">Public profile</h3>
<div class="form">
  <%= form_with(url: handle_upload_path, local: true) do |f| %>
    <div class="input">
      <%= f.file_field :image, :accept => "jpeg/jpg/png" %>
    </div>
    <span class="error"><%= @error %></span>   
    <div class="input">
      <%= f.text_field :folder, placeholder: "Folder name.Eg. img_01" %>
    </div>
    <div class="input">
      <%= f.submit "Upload" %>
    </div>
  <% end %>
</div>
